{
    "version": 2,
    "builds": [
      {
        "src": "src/back.py", 
        "use": "@vercel/python"  
      },
      {
        "src": "static/**", 
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/src/back.py" 
      }
    ]
  }
  